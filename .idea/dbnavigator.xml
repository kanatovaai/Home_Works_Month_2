create table students33 (id integer primary key unique , name text , surname text);
create table professor (
id integer primary key unique, name text , surname text , subject text
);
create table professor_student33 (
    professor_id integer , student_id integer ,
    foreign key(professor_id) references professor(id), 
    foreign key(student_id) references students33(id)
);


