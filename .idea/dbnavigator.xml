    <connection id="7091537e-5e13-44f8-a120-c5d9dba331ab">
      <console name="Connection 1" type="STANDARD" schema="main" session="Main"><![CDATA[create table author (
id integer  primary key unique , name text , surname text , work_place text 
);



create table post (
id integer primary key unique , title text , description text, 
date_1 text, author_id integer, foreign key (author_id) references author(id)
);

insert into author values (1, 'Piter', 'Parker', 'Daily Bugle')

insert into post values (1, 'awesome Spider-Man', 'kjkjkj', '2008.07.01', 1);
insert into post values (2, 'Spaider-MAn saves the city', 'jkjkjl', '2008.07.11', 1);

insert into author values (2, 'Clark', 'Kent', 'Daily Planet');

insert into post values (3, 'super-Man спасает бабушку от грузовика', 'klklkl', '2020.09.01', 2);
insert into post values (4, 'super-Man dead', 'klklkl', '2020.12.12', 2);


select * from post where author_id = 2;

select post.title , post.description, author.name, author.work_place
from post 
inner join author author on author.id = post.author_id where author_id = 1;]]></console>
    </connection>
